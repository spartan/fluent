{
    "name": "spartan/fluent",
    "description": "Spartan Fluent library",
    "type": "library",
    "keywords": [
        "library",
        "spartan",
        "fluent"
    ],
    "homepage": "https://spartanphp.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Iulian N.",
            "email": "iulian@spartanphp.com",
            "homepage": "https://spartanphp.com",
            "role": "developer"
        }
    ],
    "support": {
        "email": "support@spartanphp.com",
        "docs": "https://docs.spartanphp.com"
    },
    "require": {
        "php": ">=7.4",
        "opis/closure": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "Spartan\\Fluent\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Spartan\\Fluent\\Test\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
